{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module",
    "experimentalDecorators": true
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": "warn",
    "quotes": [2, "single"],
    "react/prop-types": 0
  }
}